{
  "name": "History++",
  "description": "Better history for (not just) developers! Find your frequent pages faster.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "bookmark popup",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "permissions": [
    "history",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}